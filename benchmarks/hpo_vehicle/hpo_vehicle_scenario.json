{
    "application_name": "hpo_vehicle",
    "optimization_objectives": ["Value"],
    "design_of_experiment": {
        "number_of_samples": 6
    },
    "optimization_iterations": 94,
    "models":{
        "model": "gaussian_process"
    },
    "input_parameters" : {
        "alpha": {
            "parameter_type" : "real",
            "values" : [-8, 0],
            "prior": "custom_gaussian"
        },
        "batch_size": {
            "parameter_type" : "real",
            "values" : [2, 8],
            "prior": "custom_gaussian"
        },
        "depth": {
            "parameter_type" : "ordinal",
            "values" : [0, 1, 2],
            "prior": [0.0003, 0.1192, 0.8805]
        },
        "learning_rate_init": {
            "parameter_type" : "real",
            "values" : [-5, 0],
            "prior": "custom_gaussian"
        },
        "width": {
            "parameter_type" : "real",
            "values" : [4, 10],
            "prior": "custom_gaussian"
        }
    }
}

